{
  "version": 2,
  "builds": [
    {
      "src": "api/faqs.js",
      "use": "@vercel/node"
    },
    {
      "src": "dynamic.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dynamic.html"
    }
  ],
  "functions": {
    "api/faqs.js": {
      "maxDuration": 10
    }
  }
}